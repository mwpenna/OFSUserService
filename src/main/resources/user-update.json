{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "user",
  "description": "JSON User Update",
  "type" : "object",
  "properties": {
    "id": {
      "not": {}
    },
    "href": {
      "not": {}
    },
    "createdOn": {
      "not": {}
    },
    "token": {
      "not": {}
    },
    "tokenExpDate": {
      "type" : ["string" , "null"], "format" : "date-time"
    },
    "activeFlag": {
      "type": "boolean"
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "company": {
      "not" : {}
    },
    "role": {
      "type": "string",
      "enum": [
        "ADMIN",
        "ACCOUNT_MANAGER",
        "WAREHOUSE",
        "CUSTOMER"
      ]
    },
    "userName": {
      "not" : {}
    },
    "password": {
      "type": "string"
    },
    "emailAddress": {
      "type": "string"
    }
  },
  "additionalProperties": true
}

